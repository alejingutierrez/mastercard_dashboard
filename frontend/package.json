{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-types": "npm run type-check && vite build",
    "type-check": "tsc -b --force",
    "type-check:watch": "tsc -b --watch",
    "type-check:clean": "tsc -b --clean && tsc -b",
    "lint": "eslint src --max-warnings 0",
    "lint:cached": "eslint src --max-warnings 0 --cache --cache-location node_modules/.cache/eslint",
    "lint:fix": "eslint src --fix",
    "lint:fix:cached": "eslint src --fix --cache --cache-location node_modules/.cache/eslint",
    "lint:debug": "ESLINT_USE_FLAT_CONFIG=true eslint src --debug",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite node_modules/.tmp node_modules/.cache",
    "clean:cache": "rm -rf node_modules/.vite node_modules/.tmp node_modules/.cache/eslint"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "antd": "^5.27.5",
    "axios": "^1.12.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.11",
    "@vitejs/plugin-react-swc": "^4.1.0"
  }
}
